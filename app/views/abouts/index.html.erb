<p id="notice"><%= notice %></p>

<h1>About Us</h1>

<table>
  <thead>
    <tr>
      <th>What we do!</th>
      <th>Phone us at:</th>
      <th>Email us at:</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @abouts.each do |about| %>
      <tr>
        <td><%= about.description %></td>
        <td><%= about.contact_email %></td>
        <td><%= about.contact_phone %></td>
      </tr>
    <% end %>
  </tbody>
</table>
